#include <iostream>
using namespace std;
//https://cses.fi/problemset/task/1083
//missing number 
// use the sum to n terms method, s_n = n(n+1)/2
// missing number is s_n minus all the rest of the numbers
int main() {
  // read integer n
  long int n; cin>> n;
  long int sum = n*(n+1)/2;
  long int m;
  while(n-1>0){
    cin>>m;
    sum = sum - m;
    n--;
  }
  cout << sum;
  return 0;
}
